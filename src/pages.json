{
  "easycom": {
    //uView
    "^u-(.*)": "uview-ui/components/u-$1/u-$1.vue",
    // uni-ui
    "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
  },
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "绘雨AI",
    "navigationBarBackgroundColor": "#000000",
    "backgroundColor": "#000000"
  },
  "tabBar": {
    "color": "#B3B3B3",
    "selectedColor": "#759EF0",
    "borderStyle": "white",
    "backgroundColor": "#1A1A1A",
    "list": [
      {
        "pagePath": "pages/index/index",
        "iconPath": "static/images/tab_draw.png",
        "selectedIconPath": "static/images/tab_draw_sel.png",
        "text": "创作"
      },
      {
        "pagePath": "pages/pic/index",
        "iconPath": "static/images/tab_folder_open.png",
        "selectedIconPath": "static/images/tab_folder_open_sel.png",
        "text": "画夹"
      },
      {
        "pagePath": "pages/me/index",
        "iconPath": "static/images/tab_me.png",
        "selectedIconPath": "static/images/tab_me_sel.png",
        "text": "我的"
      }
    ]
  },
  "pages": [
    //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index"
    },
    {
      "path": "pages/pic/index",
      "style": {
        "navigationBarTitleText": "画夹",
        "backgroundTextStyle": "light",
        "enablePullDownRefresh": true
      }
    },
    {
      "path": "pages/me/index",
      "style": {
        "navigationBarTitleText": "我的",
        "backgroundTextStyle": "light",
        "enablePullDownRefresh": true
      }
    }
  ],
  "condition": {
    "current": 0,
    "list": [
      {
        "name": "test",
        "path": "pages/pic/index"
      }
    ]
  }
}
